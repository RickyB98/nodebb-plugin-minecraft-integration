<div class="col-xs-6 col-sm-6 col-md-6 col-lg-12">
<div class="panel panel-default">
    <div class="panel-heading">{title}</div>
    <div class="status-widget">
        <table class="table table-striped">
            <tbody>
            <!-- BEGIN topPlayers -->
            <tr>
                <td class="td-label">
                    <img src="https://cravatar.eu/helmavatar/{topPlayers.player}/40" data-placement="top" data-toggle="tooltip" rel="tooltip" class="user-img" title="{topPlayers.player}" size="40" width="40" height="40" style="width: 40px; height: 40px; border-radius: 3px;" />
                </td>
                <td style="vertical-align:middle">
                    <strong>{topPlayers.minutes} Minutes</strong>
                </td>
            </tr>
            <!-- END topPlayers -->
            </tbody>
        </table>
    </div>
</div>
</div>