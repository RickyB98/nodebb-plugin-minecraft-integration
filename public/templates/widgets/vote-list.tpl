<div class="mi-container"
	data-sid="{sid}"
	data-widget="mi-vote-list"
	data-colors="{useColors}">

	<table class="table widget-table">
		<tbody>
			<!-- BEGIN services -->
			<tr>
				<td style="vertical-align:middle">
					<span>{services.name}</span>
				</td>
				<td style="vertical-align:middle;width:1px;">
					<a href="{services.url}" target="_blank" class="mi-btn-vote btn btn-success" data-service="{services.service}">Vote</a>
				</td>
			</tr>
			<!-- END services -->
		</tbody>
	</table>

</div>
